{
  "name": "task-management-system",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "node apps/api/scripts/nx.js",
    "start": "node apps/api/scripts/nx.js serve",
    "start:api": "node apps/api/scripts/nx.js serve api",
    "dev:api": "apps\\api\\start.bat",
    "dev:api:node": "node apps/api/start.js",
    "start:dashboard": "node apps/api/scripts/nx-wrapper.js serve dashboard",
    "fix-paths": "powershell -ExecutionPolicy Bypass -File apps/api/scripts/fix-paths.ps1 -Setup",
    "clean": "powershell -ExecutionPolicy Bypass -File apps/api/scripts/fix-paths.ps1 -Clean",
    "clean:all": "powershell -ExecutionPolicy Bypass -File apps/api/scripts/fix-paths.ps1 -Clean -Setup",
    "build": "node apps/api/scripts/nx-wrapper.js build",
    "test": "node apps/api/scripts/nx-wrapper.js run-many --target=test --projects=api,dashboard --output-style=stream",
    "test:api": "node apps/api/scripts/nx-wrapper.js run-many --target=test --projects=api --output-style=stream",
    "test:dashboard": "node apps/api/scripts/nx-wrapper.js run-many --target=test --projects=dashboard --output-style=stream",
    "lint": "node apps/api/scripts/nx-wrapper.js lint",
    "affected:test": "node apps/api/scripts/nx-wrapper.js affected --target=test",
    "affected:build": "node apps/api/scripts/nx-wrapper.js affected --target=build",
    "affected:lint": "node apps/api/scripts/nx-wrapper.js affected --target=lint",
    "seed": "node apps/api/scripts/run-seed.js",
    "reset:db": "node -r dotenv/config -e \"const fs=require('fs');const p=process.env.DATABASE_NAME||'task_management.db';try{fs.unlinkSync(p);console.log('Deleted '+p);}catch(e){if(e.code==='ENOENT')console.log('No file '+p);else throw e;}\""
  },
  "private": true,
